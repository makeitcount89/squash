        ]);
        const barData12 = new google.visualization.DataTable();
        barData12.addColumn('string', 'Drive');
        barData12.addColumn('number', 'W'); // Separate series for "W"
        barData12.addColumn('number', 'E'); // Separate series for "E"
        barData12.addRows([
          ['W', wCount12, 0],
          ['E', 0, eCount12]
        ]);
        const barData13 = new google.visualization.DataTable();
        barData13.addColumn('string', 'Drive');
        barData13.addColumn('number', 'W'); // Separate series for "W"
        barData13.addColumn('number', 'E'); // Separate series for "E"
        barData13.addRows([
          ['W', wCount13, 0],
          ['E', 0, eCount13]
        ]);

        const pieChart = new google.visualization.PieChart(document.getElementById('pieChart'));
        const barChart2 = new google.visualization.BarChart(document.getElementById('barChart2'));
        const barChart3 = new google.visualization.BarChart(document.getElementById('barChart3'));
        const barChart4 = new google.visualization.BarChart(document.getElementById('barChart4'));
        const barChart5 = new google.visualization.BarChart(document.getElementById('barChart5'));
        const barChart6 = new google.visualization.BarChart(document.getElementById('barChart6'));
        const barChart7 = new google.visualization.BarChart(document.getElementById('barChart7'));
        const barChart8 = new google.visualization.BarChart(document.getElementById('barChart8'));
        const barChart9 = new google.visualization.BarChart(document.getElementById('barChart9'));
        const barChart10 = new google.visualization.BarChart(document.getElementById('barChart10'));
        const barChart11 = new google.visualization.BarChart(document.getElementById('barChart11'));
        const barChart12 = new google.visualization.BarChart(document.getElementById('barChart12'));
        const barChart13 = new google.visualization.BarChart(document.getElementById('barChart13'));

        pieChart.draw(pieData, pieOptions);
        barChart2.draw(barData2, barOptions2);
        barChart3.draw(barData3, barOptions2);
        barChart4.draw(barData4, barOptions2);
        barChart5.draw(barData5, barOptions2);
        barChart6.draw(barData6, barOptions2);
        barChart7.draw(barData7, barOptions2);
        barChart8.draw(barData8, barOptions2);
        barChart9.draw(barData9, barOptions2);
        barChart10.draw(barData10, barOptions2);
        barChart11.draw(barData11, barOptions2);
        barChart12.draw(barData12, barOptions2);
        barChart13.draw(barData13, barOptions2);

        // Update total counters
        const totalWCount = wCount + wCount3 + wCount4 + wCount5 + wCount8 + wCount9 + wCount10 + wCount11 + wCount12 + wCount13;
        const totalECount = eCount + eCount3 + eCount4 + eCount5 + eCount8 + eCount9 + eCount10 + eCount11 + eCount12 + eCount13;
        document.getElementById('totalWCount').textContent = totalWCount;
        document.getElementById('totalECount').textContent = totalECount;

      }
      // Function to update total counters and localStorage
// Function to update total counters and localStorage with new names
function updateTotalCountsAndLocalStorageWithNewNames() {
  // Get the current total counts from local storage
  let totalWCount = parseInt(localStorage.getItem('totalWCountg2')) || 0;
  let totalECount = parseInt(localStorage.getItem('totalECountg2')) || 0;

  // Calculate the new totals with the desired names
  const totalWCountg2 = wCount + wCount3 + wCount4 + wCount5 + wCount8 + wCount9 + wCount10 + wCount11 + wCount12 + wCount13;
  const totalECountg2 = eCount + eCount3 + eCount4 + eCount5 + eCount8 + eCount9 + eCount10 + eCount11 + eCount12 + eCount13;

  // Update localStorage with the new total counts and names
  localStorage.setItem('totalWCountg2', totalWCountg2);
  localStorage.setItem('totalECountg2', totalECountg2);

  // Update the displayed totals on the page
  document.getElementById('totalWCountg2').textContent = totalWCountg2;
  document.getElementById('totalECountg2').textContent = totalECountg2;
}

  // Add a click event listener to all buttons on the page
  const buttons = document.querySelectorAll('button');
  buttons.forEach(button => {
  button.addEventListener('click', function() {
    // Call the function to update total counters and localStorage with new names
    updateTotalCountsAndLocalStorageWithNewNames();
    });
    });

    // Initial update when the page loads
    updateTotalCountsAndLocalStorage();

    

      

    </script>
  </body>

</html>
