      // Initialize counters
      let wCount = 0;
      let eCount = 0;
      let wCount3 = 0;
      let eCount3 = 0;
      let wCount4 = 0;
      let eCount4 = 0;
      let wCount5 = 0;
      let eCount5 = 0;
      let wCount6 = 0;
      let eCount6 = 0;
      let wCount7 = 0;
      let eCount7 = 0;
      let wCount8 = 0;
      let eCount8 = 0;
      let wCount9 = 0;
      let eCount9 = 0;
      let wCount10 = 0;
      let eCount10 = 0;
      let wCount11 = 0;
      let eCount11 = 0;
      let wCount12 = 0;
      let eCount12 = 0;
      let wCount13 = 0;
      let eCount13 = 0;

      // Load Google Charts
      google.charts.load('current', {
        'packages': ['corechart']
      });
      google.charts.setOnLoadCallback(drawCharts);

      // Define chart options
      const pieOptions = {
        
        colors: ['green', 'red']
      };

      const barOptions2 = {
        title: 'Drive vs Game 1',
        legend: {
          position: 'none'
        },
        hAxis: {
          title: 'Drive',
          minValue: 0,
          maxValue: 10
        },
        vAxis: {
          title: 'Game1',
          minValue: 0,
          maxValue: 10
        },
        series: {
          0: {
            color: 'green'
          }, // Set color for "W"
          1: {
            color: 'red'
          } // Set color for "E"
        },
        bars: 'horizontal'
      };

      // Function to draw Google Charts
      function drawCharts() {
        // Pie Chart
        const pieData = new google.visualization.DataTable();
        pieData.addColumn('string', 'Task');
        pieData.addColumn('number', 'Percentage');
        pieData.addRows([
          ['W', wCount + wCount3 + wCount4 + wCount5 + wCount6 + wCount7 + wCount8 + wCount9 + wCount10 + wCount11 + wCount12 + wCount13],
          ['E', eCount + eCount3 + eCount4 + eCount5 + eCount6 + eCount7 + eCount8 + eCount9 + eCount10 + eCount11 + eCount12 + eCount13]
        ]);

        const pieChart = new google.visualization.PieChart(document.getElementById('pieChart'));
        pieChart.draw(pieData, pieOptions);

        // Bar Charts
        const barData2 = new google.visualization.DataTable();
        barData2.addColumn('string', 'Drive');
        barData2.addColumn('number', 'W'); // Separate series for "W"
        barData2.addColumn('number', 'E'); // Separate series for "E"
        barData2.addRows([
          ['W', wCount, 0],
          ['E', 0, eCount]
        ]);

        const barChart2 = new google.visualization.BarChart(document.getElementById('barChart2'));
        barChart2.draw(barData2, barOptions2);

        const barData3 = new google.visualization.DataTable();
        barData3.addColumn('string', 'Drive');
        barData3.addColumn('number', 'W'); // Separate series for "W"
        barData3.addColumn('number', 'E'); // Separate series for "E"
        barData3.addRows([
          ['W', wCount3, 0],
          ['E', 0, eCount3]
        ]);

        const barChart3 = new google.visualization.BarChart(document.getElementById('barChart3'));
        barChart3.draw(barData3, barOptions2);

        const barData4 = new google.visualization.DataTable();
        barData4.addColumn('string', 'Drive');
        barData4.addColumn('number', 'W'); // Separate series for "W"
        barData4.addColumn('number', 'E'); // Separate series for "E"
        barData4.addRows([
          ['W', wCount4, 0],
          ['E', 0, eCount4]
        ]);

        const barChart4 = new google.visualization.BarChart(document.getElementById('barChart4'));
        barChart4.draw(barData4, barOptions2);

        const barData5 = new google.visualization.DataTable();
