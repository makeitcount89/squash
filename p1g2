      // Update Google Charts when buttons are clicked
      function updateCharts() {
        const pieData = new google.visualization.DataTable();
        pieData.addColumn('string', 'Task');
        pieData.addColumn('number', 'Percentage');
        pieData.addRows([
          ['W', wCount + wCount3 + wCount4 + wCount5 + wCount8 + wCount9 + wCount10 + wCount11 + wCount12 + wCount13],
          ['E', eCount + eCount3 + eCount4 + eCount5 + eCount8 + eCount9 + eCount10 + eCount11 + eCount12 + eCount13]
        ]
        );

        const barData2 = new google.visualization.DataTable();
        barData2.addColumn('string', 'Drive');
        barData2.addColumn('number', 'W'); // Separate series for "W"
        barData2.addColumn('number', 'E'); // Separate series for "E"
        barData2.addRows([
          ['W', wCount, 0],
          ['E', 0, eCount]
        ]);

        const barData3 = new google.visualization.DataTable();
        barData3.addColumn('string', 'Drive');
        barData3.addColumn('number', 'W'); // Separate series for "W"
        barData3.addColumn('number', 'E'); // Separate series for "E"
        barData3.addRows([
          ['W', wCount3, 0],
          ['E', 0, eCount3]
        ]);

        const barData4 = new google.visualization.DataTable();
        barData4.addColumn('string', 'Drive');
        barData4.addColumn('number', 'W'); // Separate series for "W"
        barData4.addColumn('number', 'E'); // Separate series for "E"
        barData4.addRows([
          ['W', wCount4, 0],
          ['E', 0, eCount4]
        ]);

        const barData5 = new google.visualization.DataTable();
        barData5.addColumn('string', 'Drive');
        barData5.addColumn('number', 'W'); // Separate series for "W"
        barData5.addColumn('number', 'E'); // Separate series for "E"
        barData5.addRows([
          ['W', wCount5, 0],
          ['E', 0, eCount5]
        ]);

        const barData6 = new google.visualization.DataTable();
        barData6.addColumn('string', 'Drive');
        barData6.addColumn('number', 'W'); // Separate series for "W"
        barData6.addColumn('number', 'E'); // Separate series for "E"
        barData6.addRows([
          ['W', wCount6, 0],
          ['E', 0, eCount6]
        ]);

        const barData7 = new google.visualization.DataTable();
